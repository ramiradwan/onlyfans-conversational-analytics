{
  "name": "onlyfans-analytics-frontend",
  "private": true,
  "version": "0.2.0",
  "type": "module",
  "scripts": {
    "dev": "vite",
    "build": "vite build --outDir ../app/static/dist --manifest --emptyOutDir",
    "preview": "vite preview",
    "sync:rest": "openapi-typescript http://localhost:8000/openapi.json --output src/types/backend.ts",
    "sync:wss": "curl -s http://localhost:8000/api/v1/schemas/wss | npx json2ts --stdin --output src/types/backend-wss.ts",
    "sync:all": "npm-run-all --parallel sync:rest sync:wss && node scripts/postprocess-types.js",
    "typecheck": "tsc --noEmit"
  },
  "dependencies": {
    "@emotion/react": "^11.11.0",
    "@emotion/styled": "^11.11.0",
    "@mui/icons-material": "^7.3.5",
    "@mui/material": "^7.3.5",
    "@types/react": "^18.3.26",
    "@types/react-dom": "^18.3.7",
    "react": "^18.3.1",
    "react-dom": "^18.3.1",
    "react-use-websocket": "^4.13.0",
    "zustand": "^5.0.8"
  },
  "devDependencies": {
    "@types/chrome": "^0.1.27",
    "@types/node": "^24.10.1",
    "@vitejs/plugin-react": "^4.0.0",
    "json-schema-to-typescript": "^11.0.2",
    "npm-run-all": "^4.1.5",
    "openapi-typescript": "^6.2.0",
    "typescript": "^5.9.3",
    "vite": "^5.0.0"
  }
}
